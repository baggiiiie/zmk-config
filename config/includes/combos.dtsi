/ {
   
    combos {
        compatible = "zmk,combos";
        
        combo_esc {
            timeout-ms = <50>;
            key-positions = <LT2 LT3>;
            // global-quick-tap-ms = <75>;
            layers = <0>;
            bindings = <&kp ESC>;
        };

        combo_enter {
            timeout-ms = <50>;
            key-positions = <LT1 LT2>;
            // global-quick-tap-ms = <75>;
            layers = <0>;
            bindings = <&kp ENTER>;
            require-prior-idle-ms = <200>;
        };

         combo_tab {
            timeout-ms = <30>;
            key-positions = <LM2 LM3>;
            layers = <0>;
            bindings = <&kp TAB>;
            require-prior-idle-ms = <200>;
        };

         combo_underscore {
            timeout-ms = <30>;
            key-positions = <RM1 RM2>;
            // global-quick-tap-ms = <75>;
            layers = <0>;
            bindings = <&kp UNDER>;
            require-prior-idle-ms = <100>;
        };
			   
         combo_hyphen {
            timeout-ms = <30>;
            key-positions = <LM1 LM2>;
            // global-quick-tap-ms = <75>;
            layers = <0>;
            bindings = <&kp MINUS>;
            require-prior-idle-ms = <100>;
        };

    };
};
